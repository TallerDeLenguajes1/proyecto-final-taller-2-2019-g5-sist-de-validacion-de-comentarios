        <Button Name="botonCancelar" Content="Cancelar" HorizontalAlignment="Left" Margin="111,341,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBlock HorizontalAlignment="Left" Margin="244,235,0,0" TextWrapping="Wrap" Text="Usuario" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="357,232,0,0" TextWrapping="Wrap" Text="Usuario" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.508,0.087"/>
